datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator photon {
  provider = "photonjs"
  target   = "darwin"
}

generator nexus_prisma {
  provider = "nexus-prisma"
  target   = "darwin"
}

model Banner {
  id     String @default(cuid()) @id @unique
  url    String
  imgUrl String
}